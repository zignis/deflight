{
  "name": "deflight",
  "description": "Bypass express middlewares for pre-flight requests",
  "version": "1.0.0",
  "scripts": {
    "test": "yarn jest --detectOpenHandles",
    "typecheck": "yarn tsc --project ./tsconfig.static.json",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "build": "yarn exec 'yarn typecheck && yarn format && yarn lint && yarn rimraf dist && tsc -p . && yarn rimraf dist/__tests__'",
    "prepublish": "yarn build"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort": "^2.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^29.4.3",
    "prettier": "^2.4.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "typescript-eslint-parser": "^22.0.0"
  },
  "packageManager": "yarn@3.4.1",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zignis/deflight.git"
  },
  "keywords": [
    "express",
    "preflight",
    "middleware"
  ],
  "author": {
    "name": "zignis"
  },
  "bugs": {
    "url": "https://github.com/zignis/deflight/issues"
  },
  "homepage": "https://github.com/zignis/deflight#readme"
}
