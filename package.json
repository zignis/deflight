{
  "name": "deflight",
  "description": "Bypass express middlewares for pre-flight requests",
  "version": "1.0.4",
  "type": "module",
  "scripts": {
    "test": "yarn jest --detectOpenHandles",
    "typecheck": "yarn tsc --project ./tsconfig.static.json",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "build": "rollup --config rollup.config.js",
    "prepublish": "yarn lint && yarn format && yarn build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort": "^2.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^29.4.3",
    "prettier": "^2.4.1",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "typescript-eslint-parser": "^22.0.0"
  },
  "packageManager": "yarn@3.5.0",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zignis/deflight.git"
  },
  "keywords": [
    "express",
    "preflight",
    "middleware"
  ],
  "author": {
    "name": "zignis"
  },
  "bugs": {
    "url": "https://github.com/zignis/deflight/issues"
  },
  "homepage": "https://github.com/zignis/deflight#readme"
}
